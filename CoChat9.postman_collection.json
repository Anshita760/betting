		{
			"name": "Banks",
			"item": [
				{
					"name": "Add New Bank",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"let firstName = pm.variables.replaceIn(\"{{$randomFirstName}}\")",
									"pm.variables.get(\"variable_key\");",
									"let lastName = pm.variables.replaceIn(\"{{$randomLastName}}\")",
									"let accountNo = Math.floor(Math.random()*1E16);",
									"",
									"pm.environment.set(\"firstName\", firstName)",
									"pm.environment.set(\"lastName\",lastName)",
									"pm.environment.set(\"accountNo\",accountNo)",
									"",
									"",
									"const bankNames = [\"Bank of Baroda\",\"Axis Bank\",\"HDFC Bank\",\"ICICI Bank\",\"Yes Bank\"];",
									"const random = Math.floor(Math.random() * bankNames.length);",
									"const bankName = bankNames[random];",
									"pm.environment.set(\"bankName\",bankName)",
									"",
									"const matches = bankName.match(/\\b(\\w)/g);",
									"const acronym = matches.join('');",
									"pm.environment.set(\"ifscCode\",`${acronym.toUpperCase()}000${accountNo.toString().substring(0,4)}`);",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						},
						{
							"listen": "test",
							"script": {
								"exec": [
									"var jsonData = JSON.parse(responseBody);",
									"postman.setEnvironmentVariable(\"bankGuid\", jsonData.data?.bank_guid)"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Authorization",
								"value": "{{userToken}}",
								"type": "text"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"bank_name\" : \"{{bankName}}\",\n    \"account_name\" : \"{{firstName}} {{lastName}}\",\n    \"account_no\" : \"{{accountNo}}\",\n    \"ifsc_code\" : \"{{ifscCode}}\"\n\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{base_url}}bank/add",
							"host": [
								"{{base_url}}bank"
							],
							"path": [
								"add"
